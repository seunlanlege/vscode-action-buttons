{
	"name": "action-buttons",
	"displayName": "VsCode Action Buttons",
	"description": "",
	"version": "1.1.3",
	"publisher": "seunlanlege",
	"repository": {
		"type": "git",
		"url": "https://github.com/SeunLanLege/vscode-action-buttons.git"
	},
	"engines": {
		"vscode": "^1.27.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.refreshButtons",
				"title": "Refresh Action Buttons"
			}
		],
		"configuration": {
			"type": "object",
			"title": "VsCode Action Buttons",
			"properties": {
				"actionButtons": {
					"type": "object",
					"properties": {
						"commands": {
							"type": "array",
							"items": {
								"type": "object",
								"properties": {
									"cwd": {
										"type": "string",
										"required": false
									},
									"command": {
										"type": "string",
										"required": false
									},
									"singleInstance": {
										"type": "boolean"
									},
									"name": {
										"type": "string"
									},
									"color": {
										"type": "string",
										"required": false
									}
								}
							}
						},
						"defaultColor": {
							"type": "string",
							"required": false
						},
						"loadNpmCommands": {
							"type": "boolean",
							"required": false
						}
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"publish": "vsce publish",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.32",
		"@types/node": "^6.0.40",
		"mocha": "^6.1.4",
		"typescript": "^2.0.3",
		"vscode": "^1.0.0"
	}
}
